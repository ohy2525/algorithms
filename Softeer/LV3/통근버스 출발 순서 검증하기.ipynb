   "source": [
    "n = int(input())\n",
    "arr = list(map(int, input().split()))\n",
    "\n",
    "answer = 0\n",
    "for i in range(n):\n",
    "    cnt = 0\n",
    "    for k in range(i + 1, n):\n",
    "        if arr[i] < arr[k]:\n",
    "            cnt += 1\n",
    "        else:\n",
    "            answer += cnt\n",
    "            \n",
    "print(answer)"
   ]
